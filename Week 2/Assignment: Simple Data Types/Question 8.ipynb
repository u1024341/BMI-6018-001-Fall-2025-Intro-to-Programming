    "# Susan's Body Mass Index (BMI) calculator\n",
    "\n",
    "# Weight & Height\n",
    "weight = 60 # in kilograms\n",
    "height = 1.58 # in meters\n",
    "\n",
    "# Calculate BMI\n",
    "bmi = weight / (height ** 2)\n",
    "\n",
    "# Print BMI as a float\n",
    "print(\"Susan's BMI is:\", float(bmi))\n"
